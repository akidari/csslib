n	  \n// 	  alert(\"codeAddress : \" + typeVal);\n    \n    var addVal = document.getElementsByName(typeVal);\n    \n//     	alert(addVal.length);\n//     if(!oneCheck){\n    	\n	    if( addVal.length > 0 ){\n	    	\n	    	document.getElementById(\"viewmap\").style.display = \"\";\n	    	\n	    	$(\"#map_canvas\").html(\"\");\n			\n			//맵 초기화\n			initialize();\n	    	\n	    	var address = new Array(addVal.length);\n	    	\n	    	for (var i = 0; i < addVal.length; i++) {\n// 	    		alert(i + \" : \" + document.getElementsByName(typeVal)[i].value);\n	    		if(document.getElementsByName(typeVal)[i].value == \"\"){\n	    			address[i] = \"japan\";\n	//     			address[i] = document.getElementsByName('address')[i].value;    \n	    		}else{\n	    			address[i] = document.getElementsByName(typeVal)[i].value; \n	    		}\n	        }\n	\n	    	var contentString = \"\";\n	    	var content_title = \"\";\n	    	\n	    	var address_val = \"\";\n	    	\n	    	var image = \"\";\n	    	\n	        for (var i = 0; i < address.length; i++) {\n// 	        	alert(address[i]);\n				\n	        	if( address[i].indexOf('||') > -1 ){\n    				var addrSplit = address[i].split('||');\n\n    				for( var k=0; k< addrSplit.length; k++){\n//     					alert(addrSplit[k]);\n    					\n    					if(addrSplit[k] == \"\"){\n    						addrSplit[k] = \"japan\";\n    		    		}\n    					\n    					contentString = document.getElementsByName(\"cate_nm_\" + typeVal)[i].value + \" | \"\n    					+ document.getElementsByName(\"d_cate_nm_\" + typeVal)[i].value + \"<br><br>\"\n    					+ document.getElementsByName(\"title_\" + typeVal)[i].value;\n    					\n//     					alert(contentString);\n\n						image = document.getElementsByName(\"icon_\" + typeVal)[i].value;\n    					\n    					content_title = document.getElementsByName(\"title_\" + typeVal)[i].value;\n    					address_val = addrSplit[k];\n    					\n    					addMarker(contentString, content_title, address_val, image); \n    					\n    				}\n    				\n    			}else{\n    				\n					contentString = document.getElementsByName(\"cate_nm_\" + typeVal)[i].value + \" | \"\n					+ document.getElementsByName(\"d_cate_nm_\" + typeVal)[i].value + \"<br><br>\"\n					+ document.getElementsByName(\"title_\" + typeVal)[i].value;\n					\n// 					alert(contentString);\n					image = document.getElementsByName(\"icon_\" + typeVal)[i].value;\n					\n					content_title = document.getElementsByName(\"title_\" + typeVal)[i].value;\n					address_val = address[i];\n					\n					addMarker(contentString, content_title, address_val, image); \n				\n	        	}\n	         }\n	        \n	        $(\"#map_canvas\").fadeOut(0);\n			$(\"#map_canvas\").fadeIn(800);\n			\n			document.getElementById(\"map_canvas\").focus();\n	    }\n// 	    document.getElementById(\"mapTb\").style.display = \"\";\n	    oneCheck = true;\n//     }else{\n//     	if(document.getElementById(\"mapTb\").style.display != \"\"){\n// 		 	    document.getElementById(\"mapTb\").style.display = \"\";\n// 		}else{\n// 			document.getElementById(\"mapTb\").style.display = \"none\";\n// 		}\n//     }\n  }\n  \n  //마커를 포함\n  function addMarker( contentString, content_title, address_val, image ) {\n	  	geocoder.geocode( { 'address': address_val}, function(results, status) {\n	      if (status == google.maps.GeocoderStatus.OK) {\n	        map.setCenter(results[0].geometry.location);\n	        var marker = new google.maps.Marker({\n	            map: map,\n	            position: results[0].geometry.location,\n	            icon: image,\n				title: content_title\n	        });\n	        \n// 	        alert(content_title);\n	        \n			var infowindow = new google.maps.InfoWindow({ content: contentString});\n			google.maps.event.addListener(marker, \"click\", function() {\n				infowindow.open(map,marker);\n			});\n	      } else {\n// 	    	        alert('Geocode was not successful for the following reason: ' + status);\n	      }\n	    });\n	}\n  \n//   codeAddress();\n  \n  function mapOpen(){