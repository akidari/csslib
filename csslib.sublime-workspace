elCityCart.do\" ,param ,\"json\" ,reqDelCityCart ,true);\n	}\n	\n	function reqDelCityCart(json){\n		var num = json.num;\n// 		alert(num);\n\n		if( json.result == \"ok\" ){\n			$(\"#\"+num).fadeOut(300);\n			$(\"#\"+num).remove();\n		}\n	}  \n	\n	function openPrdDetail(no){\n// 		alert($(\"#openDateDetail\"+no).css(\"display\"));\n		if( $(\"#openDateDetail\"+no).css(\"display\") == \"none\"){\n			$(\"#openDateDetail\"+no).fadeIn(300);\n		}else{\n			$(\"#openDateDetail\"+no).fadeOut(300);\n		}\n		\n	}\n	\nfunction openDetailInfo(no){\n		\n// 		if( eval(\"frmPrd\"+no).prd_date.value == ''){\n// 			alert(\"연월일을 입력하세요.\");\n// 			return;\n// 		}\n// 		$(\"#review_layer\").fadeIn(500);\n// 		$(\"#review_layer\").fadeIn(500);\n		var prd_no = eval(\"frmPrd\"+no).prd_no.value ;\n		frmCity.frmPrd_no.value = no;\n		\n		eval(\"frmPrd\"+no).prd_used_dy.value = eval(\"frmPrd\"+no).prd_date.value;\n		\n		if(eval(\"frmPrd\"+no).prd_date.value == \"\"){\n			alert(\"예약일자를 선택하세요.\");\n			return;\n		}\n		\n		if(eval(\"frmPrd\"+no).adlt_sale_cnt.value == \"\"){\n			alert(\"예약인원을 입력하세요.\");\n			eval(\"frmPrd\"+no).adlt_sale_cnt.focus();\n			return;\n		}\n		if(eval(\"frmPrd\"+no).chd_sale_cnt.value == \"\"){\n			alert(\"예약인원을 입력하세요.\");\n			eval(\"frmPrd\"+no).chd_sale_cnt.focus();\n			return;\n		}\n		if(eval(\"frmPrd\"+no).baby_sale_cnt.value == \"\"){\n			alert(\"예약인원을 입력하세요.\");\n			eval(\"frmPrd\"+no).baby_sale_cnt.focus();\n			return;\n		}\n		\n		var param = jQuery(\"#frmPrd\"+no).serialize();\n		\n		ajaxJSON(\"getCityPrdPriceList.do\" ,param ,\"html\" ,reqCityPrdPriceList ,true);\n		\n	}\n	\n	function reqCityPrdPriceList(json){\n	//		alert('cateRequest : ');\n//				alert('json : ' + json);\n// 		$(\"#review_layer\").fadeOut(0);\n		\n		$(\"#review_layer\").html(json);\n		$(\"#review_layer\").fadeIn(500);\n// 		layerpopWindow = $('#review_layer');\n// 		layerpopWindow.fadeIn(500);\n// 		\n	}\n	\n	function openDetailInfoPut(no){\n		\n// 		if( eval(\"frmPrd\"+no).prd_date.value == ''){\